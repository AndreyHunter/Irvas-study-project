{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"no-unused-vars": "warn",
		"semi": ["warn", "always"],
		"quotes": ["warn", "single"],
		"no-undef": "error",
		"no-extra-semi": "error"
	}
}
